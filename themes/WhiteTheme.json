{
    "name": "LightMode",
    "colors": {
        "editor.background": "#fff7f7",
        "editor.foreground": "#1f1a1a"
    },
    "semanticHighlighting": false,
    "tokenColors": [
        {
            "scope": [
                "keyword",
                "keyword.control",
                "storage.type",
                "keyword.control",
                "storage.modifier",
                "variable.language.special.self",
                "variable.language.self",
                "variable.language.self.luau",
                "variable.language.super",
                "variable.language.this"
            ],
            "settings": {
                "foreground": "#140f7d",
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#27cc98",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "constant.language.boolean.true",
                "constant.language.boolean.false"
            ],
            "settings": {
                "foreground": "#158c59",
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "string.quoted.single",
                "string.quoted.double",
                "string"
            ],
            "settings": {
                "foreground": "#a7ff91"
            }
        },
        {
            "scope": "entity.name.function",
            "settings": {
                "foreground": "#fafa9f"
            }
        },
        {
            "scope": [
                "constant.language",
                "number"
            ],
            "settings": {
                "foreground": "#f5ce42"
            }
        },
        {
            "scope": "punctuation",
            "settings": {
                "foreground": "#ffffff"
            }
        },
        {
            "scope": [
                "keyword.operator"
            ],
            "settings": {
                "foreground": "#f77575"
            }
        },
        {
            "scope": [
                "support.function",
                "entity.name.function   "
            ],
            "settings": {
                "foreground": "#fafa9f"
            }
        },
        {
            "scope": [
                "entity.other.attribute",
                "variable.language.metamethod",
                "variable.other.property",
                "meta.attribute",
                "meta.member.access"
            ],
            "settings": {
                "foreground": "#61a1f0"
            }
        },
        {
            "scope": [
                "entity.name.type",
                "keyword.function",
                "support.function",
                "constant.language.luau",
                "support.type"
            ],
            "settings": {
                "foreground": "#44e7f9"
            }
        },
        {
            "scope": "string.quoted",
            "settings": {
                "foreground": "#a7ff91"
            }
        },
        {
            "scope": [
                "comment.line"
            ],
            "settings": {
                "foreground": "#38a885"
            }
        },
        {
            "scope": [
                "constant.numeric",
                "constant.language.nil"
            ],
            "settings": {
                "foreground": "#f5ce42"
            }
        }
    ]
}