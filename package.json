{
  "name": "roblox-studio-like-syntax-highlighting",
  "displayName": "Roblox Studio-Like Syntax Highlighting",
  "description": "Syntax highlighting for VSCode inspired by Roblox Studio syntax highlighting. Requires Luau-lsp to work.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.98.0"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Roblox Studio-Like Syntax Highlighting",
        "uiTheme": "vs-dark",
        "path": "./themes/roblox-studio-like-syntax-highlighting.json"
      }
    ],
    "configurationDefaults": {
        "luau-lsp.platform.type": "roblox",
        "workbench.colorTheme": "Luau Roblox",
        "editor.semanticHighlighting.enabled": true
    },
    "extensionDependencies": [
        "johnnymorganz.luau-lsp",
        "ezzenix.luau-themes",
        "undermywheel.roblox-lua"
    ]
  }
}
