{
  "name": "roblox-studio-like-syntax-highlighting",
  "displayName": "Roblox Studio-Like Syntax Highlighting",
  "description": "Syntax highlighting for VSCode inspired by Roblox Studio syntax highlighting. Requires Luau-lsp to work.",
  "version": "0.1.1",
  "publisher": "ThornHall",
  "icon": "studio.png",
  "engines": {
    "vscode": "^1.98.0"
  },
  "categories": [
    "Themes"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/thornhall/roblox-studio-like-syntax-highlighting.git"
  },
  "keywords": [
    "roblox",
    "luau",
    "lua",
    "syntax highlighting",
    "roblox studio",
    "theme",
    "dark theme",
    "code highlighting",
    "color theme",
    "game dev",
    "scripting"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Roblox Studio-Like Syntax Highlighting",
        "uiTheme": "vs-dark",
        "path": "./themes/roblox-studio-like-syntax-highlighting.json"
      }
    ],
    "configurationDefaults": {
        "editor.bracketPairColorization.enabled": false,
        "luau-lsp.platform.type": "roblox"
    },
    "extensionDependencies": [
        "johnnymorganz.luau-lsp"
    ]
  }
}
